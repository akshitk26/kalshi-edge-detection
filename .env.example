# Edge Engine Environment Variables
# ==================================
# Copy this file to .env and fill in your credentials
# NEVER commit .env to version control!

# Kalshi API credentials (legacy email/password auth)
KALSHI_EMAIL=your_kalshi_email@example.com
KALSHI_PASSWORD=your_kalshi_password

# Kalshi API key auth (required for Portfolio features)
# Generate at: Kalshi Account Settings â†’ API Keys
KALSHI_API_KEY_ID=your_api_key_uuid
KALSHI_PRIVATE_KEY_PATH=~/.kalshi/private_key.pem

# OpenWeatherMap API key (get one at https://openweathermap.org/api)
OPENWEATHER_API_KEY=your_openweathermap_api_key
